#include <iostream> 
#include <vector> 

using namespace std; 

int main() 
{ 
int n,i,j,c; 
cin»n; 
vector <int> a(n); 
for(i=0;i<n;i++) 
{ 
cin»a[i]; 
} 
for(i=0;i<n;i++) 
{ 
for (int k=0;k++;) 
{ 
for(int z=1;z%k!=0;z++) 
{ 
if(i%k==z) 
{ 
for(j=0;j<n;j++) 
{ 
c=j*i; 

} 
} 

} 

cout«" "«c;} 
return 0; 
} 
}
