#include <iostream>
#include <string>
using namespace std;
int main()
{int a,b,i;
cout<<"Введите a";
cin>>a;
cout<<"Введите b";
cin>>b;
if(a<2147483648,b<2147483648,a>-2147483648,b>-2147483648){
if(a>0,b>0){
if(a<b){
for( i=0;i<=b;i+=a){
cout<<i;}}}
if(a>b){
for( i=0;i<=a;i+=b){
cout<<i;}}}
if(a<0){
for( i=0;i<=b;i-=a){
cout<<i;}}
if(b<0){
for( i=0;i<=a;i-=b){
cout<<i;}}
else {return 0;}
cout<<""<<i;
}
